{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "backpack/crud": "^6.0",
        "backpack/permissionmanager": "^7.0",
        "barryvdh/laravel-elfinder": "^0.5.2",
        "guzzlehttp/guzzle": "~7.2",
        "laravel/framework": "^10.0",
        "laravel/tinker": "^2.1",
        "laravel/ui": "^4.4",
        "livewire/livewire": "^2.5"
    },
    "require-dev": {
        "backpack/generators": "^4.0",
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "^2.13",
        "beyondcode/laravel-dump-server": "^1.9",
        "brainmaestro/composer-git-hooks": "^3.0",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.49",
        "laracasts/generators": "dev-master",
        "laravel/sail": "*",
        "mockery/mockery": "^1.6",
        "mpratt/relativetime": "^1.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "hooks": {
            "pre-commit": [
                "composer run-script php-cs-fixer",
                "composer run-script prettier",
                "git add $(git diff --cached --name-only --diff-filter=ACM -- '*.php')",
                "git add $(git diff --cached --name-only --diff-filter=ACM -- 'resources/**/*.js')"
            ],
            "post-merge": [
                "composer install",
                "yarn"
            ]
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Lib\\": "lib/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "bootstrap/view-helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": "vendor/bin/cghooks add --ignore-lock --force-win",
        "post-update-cmd": "vendor/bin/cghooks update",
        "cghooks": "vendor/bin/cghooks",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix $(git diff --cached --name-only --diff-filter=ACM -- '*.php') --stop-on-violation --config .php_cs",
        "prettier": "yarn run prettier"
    }
}
